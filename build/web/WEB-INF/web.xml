<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <servlet>
        <servlet-name>User_Reg</servlet-name>
        <servlet-class>Parking_System.User_register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User_Reg</servlet-name>
        <url-pattern>/user_reg</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>Parking_System.Admin_login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>Workers</servlet-name>
        <servlet-class>Parking_System.Worker_Add</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Workers</servlet-name>
        <url-pattern>/add_worker</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>EditWorkers</servlet-name>
        <servlet-class>Parking_System.Worker_edit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditWorkers</servlet-name>
        <url-pattern>/Worker_edit</url-pattern>
    </servlet-mapping>
   <servlet>
        <servlet-name>Space_Add</servlet-name>
        <servlet-class>Parking_System.AddSpace</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Space_Add</servlet-name>
        <url-pattern>/add_space</url-pattern>
    </servlet-mapping>
   
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
